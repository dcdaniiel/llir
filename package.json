{
  "name": "dot-collector",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "dev:debug": "nodemon --inspect src/server.js",
    "tests": "export test_persist_option='knex' && jest --runInBand --detectOpenHandles",
    "tests-memory": "export test_persist_option='memory' && jest -i",
    "seeds": "knex --env ${NODE_ENV} --knexfile knexfile.js seed:run",
    "migrations": "knex --env ${NODE_ENV} --knexfile knexfile.js migrate:latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcdaniiel/dot-collector.git"
  },
  "author": "Daniel Teixeira",
